<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
>

    <TextView
            android:layout_marginTop="0dp"
            android:text="To-Time : "
            android:layout_width="146dp"
            android:layout_height="43dp"
            android:textSize="25dp"
            android:paddingLeft="10dp"
            android:id="@+id/textView6"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="4dp" android:layout_marginBottom="288dp"
            app:layout_constraintBottom_toBottomOf="parent"/>
    <TextView
            android:text="Date : "
            android:layout_width="145dp"
            android:paddingLeft="10dp"
            android:layout_height="43dp"
            android:textSize="25dp"
            android:id="@+id/textView4" android:layout_marginBottom="4dp"
            app:layout_constraintBottom_toTopOf="@+id/textView5" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"/>
    <TextView
            android:hint="Employee Name"
            android:paddingLeft="10dp"
            android:layout_width="223dp"
            android:textSize="25dp"
            android:layout_height="43dp" android:id="@+id/textView_name"
            android:layout_alignParentEnd="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/textView_buildingname"
            app:layout_constraintStart_toEndOf="@+id/textView" android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.947"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0"/>
    <TextView
            android:hint="Room Name :"
            android:paddingLeft="10dp"
            android:layout_width="220dp"
            android:textSize="25dp"
            android:layout_height="43dp" android:id="@+id/textView_conf_name"
            android:layout_alignParentEnd="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/textView_date" app:layout_constraintStart_toEndOf="@+id/textView3"
            android:layout_marginStart="8dp" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.733"/>
    <TextView
            android:text="Room Name:"
            android:paddingLeft="10dp"
            android:layout_width="156dp"
            android:layout_height="43dp"
            android:textSize="25dp"
            android:id="@+id/textView3" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/textView4" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="4dp"/>
    <TextView
            android:hint="From-Time"
            android:layout_width="210dp"
            android:paddingLeft="10dp"
            android:layout_height="43dp"
            android:id="@+id/textView_from_time"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="16dp"
            android:textSize="25dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginBottom="12dp" app:layout_constraintBottom_toTopOf="@+id/textView_to_time"
            app:layout_constraintStart_toEndOf="@+id/textView5" android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="0.708"/>
    <TextView
            android:text="From Time :"
            android:paddingLeft="10dp"
            android:layout_width="145dp"
            android:layout_height="43dp"
            android:textSize="25dp"
            android:id="@+id/textView5" android:layout_marginBottom="12dp"
            app:layout_constraintBottom_toTopOf="@+id/textView6" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"/>
    <TextView
            android:hint="To-Time"
            android:paddingLeft="10dp"
            android:layout_width="214dp"
            android:layout_height="42dp"
            android:id="@+id/textView_to_time"
            android:textSize="25dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentStart="true"

            android:layout_above="@+id/textView_date"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="20dp" android:layout_marginBottom="64dp"
            app:layout_constraintBottom_toTopOf="@+id/editText_purpose"
            app:layout_constraintStart_toEndOf="@+id/textView6" android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="1.0"/>
    <TextView
            android:hint="Building Name"
            android:layout_width="213dp"
            android:layout_height="46dp"
            android:textSize="25dp"
            android:paddingLeft="10dp"
            android:id="@+id/textView_buildingname"

            android:layout_alignParentEnd="true"
            android:layout_alignParentStart="true"

            android:layout_above="@+id/textView_date"
            android:layout_marginBottom="4dp"
            app:layout_constraintBottom_toTopOf="@+id/textView_conf_name"
            app:layout_constraintStart_toEndOf="@+id/textView2" android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.692"/>
    <TextView
            android:hint="Booking Date"
            android:layout_width="221dp"
            android:layout_height="42dp"
            android:id="@+id/textView_date"
            android:textSize="25dp"
            android:paddingLeft="10dp"
            android:layout_alignParentEnd="true"
            android:layout_above="@+id/editText_purpose"
            android:layout_alignParentStart="true"
            android:layout_marginEnd="12dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="4dp" app:layout_constraintBottom_toTopOf="@+id/textView_from_time"
            app:layout_constraintStart_toEndOf="@+id/textView4" android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="1.0"/>

    <EditText
            android:hint="Purpose Of Meeting"
            android:layout_width="244dp"
            android:textAlignment="center"
            android:textSize="25dp"
            android:paddingLeft="10dp"
            android:layout_height="49dp"
            android:id="@+id/editText_purpose"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="71dp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            android:layout_alignParentEnd="true"
            android:layout_alignParentStart="true" android:layout_marginStart="95dp"
            android:layout_above="@+id/book_button"
            app:layout_constraintBottom_toTopOf="@+id/button" android:layout_marginBottom="36dp"
            android:singleLine="true"/>

    <!--<EditText android:layout_width="wrap_content"-->
              <!--android:layout_height="wrap_content"-->
              <!--android:hint="Purpose of Meeting11111111"-->
              <!--android:id="@+id/qwerty"/>-->
    <Button
            android:text="BOOK"
            android:layout_width="152dp"

            android:layout_height="48dp"
            android:textSize="25dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/book_button" android:layout_alignParentBottom="true"
            android:layout_marginEnd="119dp" android:layout_alignParentStart="true" android:layout_marginStart="140dp"
            android:layout_alignParentEnd="true" app:layout_constraintHorizontal_bias="1.0"
            android:layout_marginBottom="32dp" app:layout_constraintBottom_toBottomOf="parent"
    />
    <TextView
            android:text="Name : "
            android:layout_width="145dp"
            android:layout_height="43dp"
            android:textSize="25dp"
            android:paddingLeft="10dp"
            android:id="@+id/textView" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/textView2" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0"/>
    <TextView
            android:text="Building"
            android:layout_width="152dp"
            android:layout_height="43dp"
            android:textSize="25dp"
            android:paddingLeft="10dp"
            android:id="@+id/textView2" android:layout_marginBottom="0dp"
            app:layout_constraintBottom_toTopOf="@+id/textView3" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="4dp"/>
    <Button
            android:text="Select Members"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/button" android:layout_marginBottom="24dp"
            app:layout_constraintBottom_toTopOf="@+id/book_button" app:layout_constraintHorizontal_bias="0.498"/>
</androidx.constraintlayout.widget.ConstraintLayout>