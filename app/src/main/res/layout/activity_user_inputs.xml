    <RelativeLayout
            android:id="@+id/time"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true">
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="From Time"
                android:layout_marginTop="150dp"
                android:textColor="@color/common_google_signin_btn_text_dark_focused"
                android:layout_below="@+id/from_time"
                android:layout_alignParentStart="true"
                android:layout_marginStart="31dp"/>

        <EditText
                android:layout_width="100dp"
                android:layout_height="50dp"
                android:layout_marginTop="170dp"
                android:id="@+id/fromTime"
                android:inputType="none"
                android:focusable="false"
                android:paddingLeft="10dp"
                android:layout_marginLeft="50dp"
                android:layout_marginRight="90dp"
                android:textColor="@color/common_google_signin_btn_text_dark_focused"
                android:background="@drawable/edittext_bg"
        />
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="To Time"
                android:layout_marginTop="150dp"
                android:textColor="@color/common_google_signin_btn_text_dark_focused"
                android:layout_alignParentStart="true"
                android:layout_marginStart="190dp"/>
        <EditText
                android:layout_width="100dp"
                android:layout_height="50dp"
                android:layout_marginTop="170dp"
                android:id="@+id/toTime"
                android:focusable="false"

                android:paddingLeft="10dp"
                android:layout_marginStart="200dp"
                android:layout_marginLeft="10dp"
                android:textColor="@color/common_google_signin_btn_text_dark_focused"
                android:background="@drawable/edittext_bg"
                android:inputType="none"
        />
    </RelativeLayout>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Date"
            android:id="@+id/datetext"
            android:layout_marginTop="220dp"
            android:textColor="@color/common_google_signin_btn_text_dark_focused"
            android:layout_below="@+id/from_time"
            android:layout_alignParentStart="true"
            android:layout_marginStart="30dp"/>
    <EditText
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:textColor="@color/common_google_signin_btn_text_dark_focused"
            android:layout_marginTop="0dp"
            android:layout_marginStart="50dp"
            android:layout_marginEnd="50dp"
            android:id="@+id/date"
            android:focusable="false"
            android:paddingLeft="10dp"
            android:layout_marginLeft="10dp"
            android:layout_below="@id/datetext"
            android:layout_marginRight="10dp"
            android:background="@drawable/edittext_bg"
            android:editable="false"
    />
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Capacity"
            android:layout_marginTop="300dp"
            android:textColor="@color/common_google_signin_btn_text_dark_focused"
            android:textColorHint="@color/black"
            android:layout_below="@+id/from_time"
            android:focusable="false"
            android:layout_alignParentStart="true"
            android:layout_marginStart="30dp"/>


    <Spinner
            android:layout_width="wrap_content"
            android:layout_height="48dp"
            android:layout_marginEnd="50dp"
            android:id="@+id/spinner2"
            android:popupBackground="#D2D2DA"
            android:layout_marginStart="50dp"
            android:backgroundTint="#9596A0"
            android:background="#9596A0"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:visibility="visible"
            android:layout_marginTop="325dp"
            android:layout_alignParentTop="true"/>
    <Button
            android:text="NEXT"
            android:layout_width="135dp"
            android:layout_height="52dp"
            android:layout_below="@+id/spinner2"
            android:id="@+id/next"
            android:background="@drawable/buttonsubmit"
            android:layout_marginTop="89dp"
            android:layout_marginStart="112dp" android:layout_alignParentEnd="true" android:layout_marginEnd="134dp"/>


</RelativeLayout>